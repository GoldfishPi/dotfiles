!--------------------xdefauls
!URxvt.font: xft:mono:pixelsize=16
URxvt.font: xft:DejaVu Sans Mono for Powerline:pixelsize=16:style=Book
!urxvt*termName: Terminal
urxvt*scrollBar: false
urxvt*matcher.button: 1
urxvt.transparent: true
urxvt.boldFont:
URxvt.letterSpace: -1
Xft*dpi: 96
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
URxvt*cursorUnderline: true
Xft*rgba: rgb
URxvt*geometry: 85x20
*internalBorder: 0
URxvt*fading: 0
URxvt*tintColor: #ffffff
URxvt*shading: 20
URxvt*inheritPixmap: False

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c900
urxvt*blurRadius: 5

! special
*.foreground:   #c5c8c6
*.background:   #000032
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a85377
*.color9:       #a85377

! green
*.color2:       #746f81
*.color10:      #746f81

! yellow
*.color3:       #41F0FD
*.color11:      #41F0FD

! blue
*.color4:       #FD6073
*.color12:      #FD6073
! magenta
*.color5:       #764970
*.color13:      #764970

! cyan
*.color6:       #abb9ba
*.color14:      #abb9ba

! white
*.color7:       #899298
*.color15: #899298

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
